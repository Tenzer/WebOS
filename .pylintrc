[MASTER]
persistent = no

[MESSAGES CONTROL]
disable =
    duplicate-code,
    import-error,
    invalid-name,
    line-too-long,
    missing-class-docstring,
    missing-function-docstring,
    missing-module-docstring,
    no-self-use,
    relative-beyond-top-level,
    too-few-public-methods,
    too-many-branches,
    unused-argument,

[REPORTS]
reports = no
score = no
